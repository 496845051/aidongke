/**app.wxss**/
@import './asserts/css/public';
@import './asserts/css/bg';
@import './asserts/icon/iconfont.wxss';

page{
  min-height: 100%;
  font-size: 14px;
  background-color: #eeeeee;
  font-family: 'Microsoft YaHei'
}
image{
  width: 100%;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
@font-face {
  font-family: 'iconfont';  /* project id 794351 */
  src: url('//at.alicdn.com/t/font_794351_29ly0jbyoix.eot');
  src: url('//at.alicdn.com/t/font_794351_29ly0jbyoix.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_794351_29ly0jbyoix.woff') format('woff'),
  url('//at.alicdn.com/t/font_794351_29ly0jbyoix.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_794351_29ly0jbyoix.svg#iconfont') format('svg');
}
.iconfont{
  font-family:"iconfont" !important;
  font-size:16px;font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;}
